{
    "workbench.editor.untitled.hint": "hidden",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": false,
        "**/.factorypath": false,
        "**/.project": false,
        "**/.settings": false,
        "**/*.csproj": true
    },
    "workbench.colorTheme": "GitHub Dark Default",
    "explorer.confirmDelete": false,
    "code-runner.runInTerminal": true,
    "code-runner.executorMap": {
    
    
        "cpp": "cd $dirWithoutTrailingSlash && g++ -std=c++11 $fileName -o $fileNameWithoutExt && ./$fileNameWithoutExt"
    },
    "code-runner.terminalRoot":"/mnt/",
    "files.autoSave": "afterDelay",
    "C_Cpp.default.includePath": [
        "\"C:\\MinGW\\include\""
    ],

    "explorer.confirmDragAndDrop": false,
    "sonarlint.rules": {
        "java:S1481": {
            "level": "off"
        },
        "java:S1854": {
            "level": "off"
        },
        "java:S1128": {
            "level": "off"
        },
        "java:S3252": {
            "level": "off"
        },
        "java:S106": {
            "level": "off"
        },
        "java:S1192": {
            "level": "off"
        },
        "Web:S5254": {
            "level": "off"
        },
        "java:S2583": {
            "level": "off"
        },
        "Web:TableHeaderHasIdOrScopeCheck": {
            "level": "off"
        }
    },
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "debug.onTaskErrors": "debugAnyway",
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-17",
            "path": "C:\\Program Files\\Java\\jdk-17.0.1",
            "default": true
        }
    ],
    "tabnine.experimentalAutoImports": true,
    "security.workspace.trust.untrustedFiles": "open",
    "svg.preview.mode": "svg",
    "sonarlint.pathToNodeExecutable": "C:\\\\Program Files\\\\nodejs\\\\node.exe",
    "redhat.telemetry.enabled": false,
    "vscode-edge-devtools.mirrorEdits": true,
    "vscode-edge-devtools.themes": "Dark",
    "vscode-edge-devtools.browserFlavor": "Stable",
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": true,
    "git.confirmSync": false,
    "terminal.external.windowsExec": "C:\\Program Files\\WindowsApps\\Microsoft.WindowsTerminal_1.11.3471.0_x64__8wekyb3d8bbwe\\wt.exe",
    "background.enabled": false,
    "editor.inlineSuggest.enabled": true,
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.fontFamily": "DroidSansMono Nerd Font",
    "terminal.integrated.rendererType": "canvas",

    "cSpell.userWords": [
        "Behaviour"
    ],
    "omnisharp.useGlobalMono": "always", 
    "omnisharp.path": "latest",
    "git.enableSmartCommit": true,
    "csharp.referencesCodeLens.enabled": false,
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "git.autofetch": true,
    "vscode-edge-devtools.webhint": false,
    "debug.disassemblyView.showSourceCode": false,
    "terminal.external.linuxExec": "konsole",
    "google-translate.firstLanguage": "en",
    "google-translate.secondLanguage": "ru ",
    "google-translate.serverDomain": "https://translate.google.com",
    "hexeditor.columnWidth": 16,
    "hexeditor.showDecodedText": false,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "telemetry.telemetryLevel": "off",
